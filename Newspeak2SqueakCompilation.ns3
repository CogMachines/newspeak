signedSingleExtendB: extendedIndex = (

	assert: [extendedIndex between: -128 and: 127]
		message: 'Single extend B out of range'.
	
	code
		byte: 225;
		byte: (extendedIndex >= 0 
			ifTrue: [extendedIndex]
			ifFalse: [extendedIndex + 256]) 
