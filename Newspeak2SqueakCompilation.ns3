pushNil = (
	unsignedSingleExtendB: 2.
	code byte: 77.
	incrementStackDepthBy: 1.	
)
pushSelf = (
	code byte: 76.
	incrementStackDepthBy: 1.
)
pushThisContext = (
	unsignedSingleExtendB: 3.
	code byte: 77.
	incrementStackDepthBy: 1.
