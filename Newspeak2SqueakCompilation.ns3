class CodeGenerator = (|
code = LowLevelMethodMirror new.

numTemps ::= 1. (* temp0 always allocated *) (*why?*)
maxStackDepth ::= 0.
currentStackDepth ::= 0.
|)
('as yet unclassified'
argCount: n <Integer> = (
	code argCount: n
)
bci ^<Integer> = (
	(* Answers the current bytecode index. Used for calculating jump lengths. Note this is NOT the pc. *)
	^code size
)
close ^<LowLevelMethodMirror> = (
	(* To be called at the end of code generation. *)
	| methodContextSize = maxStackDepth + code argCount + code maxLocals. |
	code maxStack: (methodContextSize max: maxClosureContextSize).
	^code
)
literals: ls = (
    code literals: ls.
)
maxLocals ^<Integer> = (
	^code maxLocals
)
maxLocals: n <Integer> = (
	code maxLocals: n
)
methodName: n <Symbol> = (
	code selector: n
)
pushMixinClass = (
(* The mixin class will be stored in the final literal -added after the end of the literal array *)
	pushLiteralWithIndex: literalTable size + 1
)'stack'
decrementStackDepthBy: n = (
	(* Reduce the current height of the operand stack *)
	currentStackDepth:: currentStackDepth - n.
	assert: [currentStackDepth >= 0] 
		message: 'Stack depth must not be negative'.
)
ensureStackDepth: targetDepth <Integer> = (

	assert: [targetDepth <= currentStackDepth]
		message: 'Stack shorter than expected'.
	
	[currentStackDepth > targetDepth] whileTrue: [pop]
)
incrementStackDepthBy: n = (
	(* Increase the current height of the operand stack. Keep track of the maximum operand stack height seen in this method (that's the whole point of tracking the stack height - we need this to create a CompiledMethod. *)
	currentStackDepth:: currentStackDepth + n.
	maxStackDepth:: maxStackDepth max: currentStackDepth.
)) : ()
class CodeGeneratorV3 = CodeGenerator (|
slotsPerMethod = 64.
specialSelectors = 
	{#+. #-. #<. #>. #<=. #>=. #=. #~=. #*. #/. #\\. #@. 
	#bitShift:. #//. #bitAnd:. #bitOr:. 
	#at:. #at:put:. #size. #next. #nextPut:. #atEnd.
	#==.
	#class. #blockCopy:. #value. #value:. #do:. #new. #new:. #x. #y}.
|)
('as yet unclassified'
indexForLiteral: literal = (
	^literalTable at: literal
)
patchJumpIfAt: index <Integer> with: targetData <Integer> = (
	code byteAt: index - 1 
	      put: ((code byteAt: index - 1) bitOr: ((targetData bitAnd: 16r300)  >> 8)).
	code byteAt: index
		put: ((code byteAt: index) bitOr: (targetData bitAnd: 16rFF))
)
pushRemoteTemporary: index inVector: vectorIndex = (
	assert: [index between: 0 and: 255]
		message: 'Temp index out of range'.
	assert: [vectorIndex between: 0 and: 255]
		message: 'Temp vector index out of range'.
	
	code byte: 140; byte: index; byte: vectorIndex.
	incrementStackDepthBy: 1.
)
sizeOfBackJump = (
	^2
)
storeIntoRemoteTemporary: index inVector: vectorIndex = (
	assert: [index between: 0 and: 255]
		message: 'Temp index out of range'.
	assert: [vectorIndex between: 0 and: 255]
		message: 'Temp vector index out of range'.
	
	code byte: 141; byte: index; byte: vectorIndex.
)'jumps'
jumpBy: distance <Integer>  = (

	(* always generates long jumps *)

	assert: [distance between: -1024 and: 1023]
		message: 'Unconditional jump distance out of range'.
      code
		byte: 160 + (distance + 1024 bitShift: -8);
		byte: distance + 1024 \\ 256.
)
jumpIf: bool <Boolean> by: distance <Integer> = (

	(* always generates long jumps *)

	| popAndJump <Integer> |
	assert: [distance between: 0 and: 1023]
		message: 'Conditional jump distance out of bounds'.
	decrementStackDepthBy: 1.
	popAndJump:: bool ifTrue: [168] ifFalse: [172].
	code 
		byte: popAndJump + (distance bitShift: -8);
		byte: (distance bitAnd: 16rFF).
)
patchClosureJumpAt: index <Integer> with: targetData <Integer> = (

	code byteAt: index - 1 
		put: ((code byteAt: index - 1) bitOr: ((targetData bitAnd: 16r700)  >> 8)).
	code byteAt: index 
		put: ((code byteAt: index) bitOr: (targetData bitAnd: 16rFF)).
)
patchJumpAt: index <Integer> with: targetData <Integer> = (

	code byteAt: index - 1 
		put: ((code byteAt: index - 1) bitOr: ((targetData bitAnd: 16r700)  >> 8)).
	code byteAt: index 
		put: ((code byteAt: index) bitOr: (targetData bitAnd: 16rFF)).
)'private'
pushInteger: value = (
	code byte: 112 + 3 + 2 + value.
	incrementStackDepthBy: 1.
)
pushLiteralWithIndex: index = (

	assert: [index between: 0 and: 255]
		message: 'Literal index out of range'.
		
	incrementStackDepthBy: 1.
	
	index < 32 ifTrue: [code byte: 32 + index. ^self].
	index < 64 ifTrue: [code byte: 128; byte: 128 + index. ^self].
	code byte: 132; byte: 96; byte: index.
)'returns'
returnBooleanFromMethod: bool = (
	code byte: (bool ifTrue: [121] ifFalse: [122]).
)
returnFromBlock = (
	code byte: 125.
	decrementStackDepthBy: 1.
)
returnFromMethod = (
	code byte: 124.
	decrementStackDepthBy: 1.
)
returnSelfFromMethod = (
	code byte: 120.
)'sends'
normalSend: selector numArgs: numArgs = (
	|
	index ::= specialSelectors indexOf: selector.
	|
	decrementStackDepthBy: selector numArgs + 1 (* the receiver *).
	incrementStackDepthBy: 1.
		
	(* Special selector? *)
	index = 0 ifFalse: 
		[code byte: 175 + index.
		^self].
	index:: indexForLiteral: selector.
	
	assert: [numArgs between: 0 and: 31]
		message: 'numArgs out of range'.
	assert: [index between: 0 and: 255]
		message: 'selector literal index out of range'.
	
	(index < 16 and: [numArgs < 3]) ifTrue:
		[code byte: 208 + (numArgs * 16) + index.
		^self].
	
	(index < 32 and: [numArgs < 8]) ifTrue: 
		[code byte: 131; byte: ((numArgs bitShift: 5) + index).
		^self].
	
	(index < 64 and: [numArgs < 4]) ifTrue: 
	 	[code
			byte: 134;
			byte: ((numArgs bitShift: 6) + index).
		 ^self].
	
	code byte: 132; byte: numArgs; byte: index.
)
superSend: selector numArgs: numArgs = (
	|
	selectorIndex = indexForLiteral: selector.
	|
	assert: [selectorIndex between: 0 and: 255]
		message: 'Selector index out of range'.
	assert: [numArgs between: 0 and: 255]
		message: 'Message arity out of range'.
		
	decrementStackDepthBy: numArgs + 1 (* the receiver *).
	incrementStackDepthBy: 1.
	
	code byte: 126; byte: numArgs; byte: selectorIndex.
)'stack'
dup = (
	code byte: 136.
	incrementStackDepthBy: 1.	
)
pop = (
	code byte: 135.
	decrementStackDepthBy: 1.	
)'temporaries'
popIntoRemoteTemporary: index inVector: vectorIndex = (
	assert: [index between: 0 and: 255]
		message: 'Temp index out of range'.
	assert: [vectorIndex between: 0 and: 255]
		message: 'Temp vector index out of range'.
	
	code byte: 142; byte: index; byte: vectorIndex.
	decrementStackDepthBy: 1.
)
popIntoTemporary: index = (
	assert: [index between: 0 and: 63] message: 'Temp index out of range'.
		
	index < 8 ifTrue:
		[code byte: 104 + index.
		decrementStackDepthBy: 1.
		^self].
	
	code byte: 130; byte: 64 + index.
	decrementStackDepthBy: 1.
)
pushTemporary: index = (
	assert: [index between: 0 and: 63] message: 'Temp index out of range'.
	
	index < 16 ifTrue:
		[incrementStackDepthBy: 1.
		code byte: 16 + index.
		^self].
	
	incrementStackDepthBy: 1.
	code byte: 128; byte: 64 + index.
)
storeIntoTemporary: index = (
	assert: [index between: 0 and: 63] message: 'Temp index out of range'.
	
	code byte: 129; byte: 64 + index.
)'testing'
hasSpecialReturnBoolean = (
	^true yourself
)
isImplicitReceiverSendAbsentReceiver = (
	^false
)
isSuperSendAbsentReceiver = (
	^false
)'values'
createClosureOfArity: numArgs copying: numCopied length: jumpSize = (

	| numExtensions numCopiedMod8 numArgsMod8 extA |
	
	decrementStackDepthBy: numCopied.
	incrementStackDepthBy: 1.
	
	assert: [jumpSize between: 0 and: 65535]
		message: 'Closure jump out of range'.
	assert: [numCopied between: 0 and: 15]
		message: 'Too many copied values in closure'.
	assert: [numArgs between: 0 and: 15]
		message: 'Too many args in closure'.
	
	code
		byte: 143;
		byte: numArgs + (numCopied bitShift: 4);
		byte: (jumpSize bitShift: -8);
		byte: (jumpSize bitAnd: 16rFF)
)
createEmptyArray: size = (
	assert: [size between: 0 and: 127] message: 'Array size must be < 128'.
	incrementStackDepthBy: 1.
	code byte: 138; byte: size.
)
createFullArray: size = (
	assert: [size between: 0 and: 127] message: 'Array size must be < 128'.
	decrementStackDepthBy: size.
	incrementStackDepthBy: 1.
	code byte: 138; byte: 128 + size.
)
pushEnclosingObject: depth = (
	assert: [depth between: 0 and: 127]
		message: 'Enclosing object depth out of range'.
	
	depth = 0 ifTrue: [^pushSelf].
	
	incrementStackDepthBy: 1.
	
	code byte: 139; byte: (indexForLiteral: depth). (*What?!*)
)
pushFalse = (
	code byte: 114.
	incrementStackDepthBy: 1.
)
pushImplicitReceiver: selector  = (

	| index = indexForLiteral: selector. |
	assert: [index between: 0 and: 255]
		message: 'Selector index out of bounds'.
	
	incrementStackDepthBy: 1.
	code byte: 127; byte: index.
)
pushLiteral: value = (
	value == true ifTrue: [^pushTrue].
	value == false ifTrue: [^pushFalse].
	value == nil ifTrue: [^pushNil].
	(value isInteger and: [value between: -1 and: 2])
		ifTrue: [^pushInteger: value].
	^pushLiteralWithIndex: (indexForLiteral: value).
)
pushNil = (
	code byte: 115.
	incrementStackDepthBy: 1.
)
pushSelf = (
	code byte: 112.
	incrementStackDepthBy: 1.
)
pushThisContext = (
	code byte: 137.
	incrementStackDepthBy: 1.
)
pushTrue = (
	code byte: 113.
	incrementStackDepthBy: 1.
)) : ()
